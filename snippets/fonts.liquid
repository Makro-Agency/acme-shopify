
  <style>
  @font-face {
	font-family: 'Sierra';
	src: url('{{ "Sierra.eot" | asset_url }}');
	src: url('{{ "Sierra.eot" | asset_url }}?#iefix') format('embedded-opentype'),
    	url('{{ "Sierra.woff2" | asset_url }}') format('woff2'),
		url('{{ "Sierra.woff" | asset_url }}') format('woff'),
		url('{{ "Sierra.ttf" | asset_url }}')  format('truetype'), 
		url('{{ "Sierra.svg" | asset_url }}#sierra') format('svg'); 
	}
	@font-face {
		font-family: 'icomoon';
		src:  url('{{ "icomoon.eot" | asset_url }}');
		src:  url('{{ "icomoon.eot" | asset_url }}#iefix') format('embedded-opentype'),
		  url('{{ "icomoon.woff2" | asset_url }}') format('woff2'),
		  url('{{ "icomoon.ttf" | asset_url }}') format('truetype'),
		  url('{{ "icomoon.woff" | asset_url }}') format('woff'),
		  url('{{ "icomoon.svg" | asset_url }}#icomoon') format('svg');
		font-weight: normal;
		font-style: normal;
		}
  </style>
